<div class="container">
  <% @memos.each do |memo| %>
    <div class="card">
      <%= memo.title %>
      <%= memo.content %>
      <%= link_to "削除する", memo_path(memo.id),method: :delete %>
    </div>
  <% end %>

  <%= form_with model: @memo do |f| %>
    <%= f.text_field :title %>
    <%= f.text_field :content %>
    <%= f.submit "投稿する" %>
  <% end %>

</div>